<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form" xmlns:i="/issues">

  <f:entry title="${%Name}" field="name">
    <f:textbox default="${descriptor.getDisplayName()}"/>
  </f:entry>
  <f:entry field="hideCleanJobs">
    <f:checkbox title="${%hide.title}"/>
  </f:entry>
  <f:entry title="${%Height of the chart}" field="height">
    <f:number default="400"/>
  </f:entry>
  <i:select-tools/>

</j:jelly>
