<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:c="/charts" xmlns:i="/issues">

 <j:choose>
   <j:when test="${from.noWarnings()}">

     <c:trend-chart it="${from}" title="${from.trendName}" enableLinks="true"/>

   </j:when>
   <j:otherwise>

     <i:zero-issues/>

   </j:otherwise>

 </j:choose>

</j:jelly>
